{
  "name": "graph-data-analyzer",
  "version": "1.0.0",
  "description": "Graph analysis tool for security incident data",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/index.js",
    "dev": "ts-node src/index.ts",
    "export-md": "npm run build && node dist/index.js --markdown --output=analysis-report.md",
    "print-md": "npm run build && node dist/index.js --markdown"
  },
  "dependencies": {
    "cli-table3": "^0.6.5",
    "console.table": "^0.10.0",
    "csv-parse": "^5.5.2",
    "graphology": "^0.26.0",
    "graphology-types": "^0.24.8",
    "markdown-table": "^3.0.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
